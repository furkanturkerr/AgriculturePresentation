@model System.Collections.Generic.List<Entity.Concrate.Team>

<div class="team" id="team">
	<div class="container">
		<h3 class="tittle">Ekip Arkadaşlarımız</h3>
		<p class="heading-btm-w3ls">Birbirinden farklı konularda oldukça donanımlı ekip arkadaşlaımız.</p>
		<div class="wthree_team_grids">
			@foreach (var item in Model)
			{
				<div class="col-md-3 wthree_team_grid">
					<div class="hovereffect">
						<img src="@item.Image" alt=" " class="img-responsive"/>
						<div class="overlay">
							<h6>@item.PersonName</h6>
							<div class="rotate">
								<p class="group1">
									<a href="#">
										<i class="fa fa-twitter"></i>
									</a>
									<a href="#">
										<i class="fa fa-facebook"></i>
									</a>
								</p>
								<hr>
								<hr>
								<p class="group2">
									<a href="#">
										<i class="fa fa-instagram"></i>
									</a>
									<a href="#">
										<i class="fa fa-dribbble"></i>
									</a>
								</p>
							</div>
						</div>
					</div>
					<h4>@item.PersonName</h4>
					<p>@item.Title</p>
				</div>
			}
			<div class="clearfix"> </div>
		</div>
	</div>
</div>
