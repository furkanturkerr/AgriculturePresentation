@model System.Collections.Generic.List<Entity.Concrate.Service>

<div class="sevices-w3layouts" id="services">
    <div class="container">
        <h3 class="tittle">Hizmetlerimiz</h3>
        <p class="heading-btm-w3ls">4 Farklı hizmet türü ile sizlere ulaşmaya devam ediyoruz.</p>
        <div class="services-main">
            @foreach (var item in Model)
            {
            <div class="col-md-3 sevices-w3layouts-grids wow fadeInUp animated" data-wow-delay=".5s">
                <h5>@item.Description</h5>
                <p>@item.Title</p>

                <div class="sevices-w3layouts-img c1">
                    <i class="@item.Image" aria-hidden="true"></i>
                    <p>Lorem ipsum</p>
                </div>
            </div>
            }
        </div>
        <div class="clearfix"> </div>
    </div>
</div>