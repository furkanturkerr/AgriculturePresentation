@model System.Collections.Generic.List<Entity.Concrate.Adress>

@{
    Layout = "AdminLayout";
}

<div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Adres</h4>
            <div class="table-responsive">
                <table class="table table-striped">
                    <thead>
                    <tr>
                        <th>Açıklama1</th>
                        <th>Açıklama2</th>
                        <th>Açıklama3</th>
                        <th>Açıklama4</th>
                        <th>Düzenle</th>
                    </tr>
                    </thead>
                    <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>@item.Description1</td>
                            <td>@item.Description2</td>
                            <td>@item.Description3</td>
                            <td>@item.Description4</td>
                            <td><a href="Address/UpdateAddress/@item.AdressId" class="btn btn-outline-success">Düzenle</a></td>
                        </tr>
                    }
                    </tbody>
                </table>
                @ViewBag.Message
            </div>
        </div>
    </div>
</div>